{
  "name": "@bitecs/serialize",
  "version": "0.0.1",
  "description": "SoA serialization library",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Web-ECS/SoA-serializer.git"
  },
  "keywords": [
    "SoA",
    "serialization",
    "ECS"
  ],
  "author": "Nathaniel Martin",
  "license": "MPL-2.0",
  "scripts": {
    "test": "bun test",
    "build": "bun build ./src/index.ts --outdir ./dist && bun run build:declaration",
    "build:declaration": "tsc --outdir ./dist"
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "tsconfig": "workspace:*"
  }
}
